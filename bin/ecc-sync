#!/bin/bash

mkdir -p tmp
openssl aes-256-cbc -K $encrypted_461274fed2c2_key -iv $encrypted_461274fed2c2_iv -in .travis/travis.enc -out tmp/travis -d

chmod 600 tmp/travis # this key should have push access
ssh-add tmp/travis
git remote add ecc https://pquinn@git.earthdata.nasa.gov/scm/edsc/earthdata-search-client_repo.git
git push ecc master:deploy
