      <section id="granule-list" data-bind="visible: ui.datasetsList.focused()">
        <a class="master-overlay-close master-overlay-hide" href="#" title="close"><i class="fa fa-times-circle"></i></a>
        <nav class="master-overlay-nav">
          <a class="master-overlay-back master-overlay-hide-secondary" href="#" data-bind="click: ui.datasetsList.unfocusDataset">
            <i class="edsc-icon-arrow-left"></i> Back to Datasets
          </a>
        </nav>
        <!-- ko if: ui.datasetsList.focused() -->
        <!-- ko with: ui.datasetsList.focused() -->
        <header>
          <h2 data-bind="text: dataset.title"></h2>
        </header>
        <div class="master-overlay-info">
          <!-- ko if: granules.isLoaded() -->
          <p>
            Showing <span data-bind="text: granules.results().length"></span> of
            <span data-bind="visible: granules.hitsEstimated() && granules.hasNextPage()">approximately</span>
            <span data-bind="text: granules.hits() == 1 ? '1 matching granule' : granules.hits() + ' matching granules'"></span>
          </p>
          <!-- /ko -->
        </div>
        <div class="master-overlay-content panel-list" data-bind="event: {scroll: scrolled}">
          <ul id="project-datasets-list" class="panel-list-list" data-bind="foreach: granules.results()">

            <li class="panel-list-item" data-bind="css: {'panel-list-item-with-thumb': browse_flag === 'true', 'panel-list-focused': $parent.focused() === $data, 'panel-list-selected': $parent.isStickied($data)}, event: { mouseenter: $parent.onGranuleMouseover, mouseleave: $parent.onGranuleMouseout, click: $parent.toggleStickyFocus}">
              <!-- ko if: browse_flag === 'true' -->
                <a class="panel-list-thumbnail-container" data-bind="attr: {href: edsc_full_browse_url()}" target="_blank" title="View full image">
                  <img class="panel-list-thumbnail" data-bind="attr: {src: edsc_browse_url(65)}" alt="Browse Image">
                </a>
              <!-- /ko -->
              <a class="panel-list-remove panel-list-when-selected" href="#" title="remove"><i class="fa fa-times"></i></a>

              <h3 data-bind="text: title"></h3>
              <span data-bind="text: getTemporal()"></span>
            </li>
          </ul>
          <p class="busy panel-list-load-more" data-bind="visible: granules.isLoading()">
            Loading granules...
          </p>
        </div>
        <!-- /ko -->
        <!-- /ko -->
      </section>
