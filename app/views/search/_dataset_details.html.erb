<section data-bind="with: datasets.details()" id='dataset-details' class='master-overlay-details'>
  <header>
    <a class="master-overlay-close master-overlay-details-close" href="#"><i class="fa fa-times-circle"></i></a>
    <h2 data-bind="text: dataset_id"></h2>
  </header>
  <dl class='master-overlay-content'>
    <dt>Description:</dt>
    <dd data-bind="text: description"></dd>
    <dt>Archive Center:</dt>
    <dd data-bind="text: archive_center"></dd>
    <dt>Processing Center:</dt>
    <dd data-bind="text: processing_center"></dd>
    <dt>Short Name:</dt>
    <dd data-bind="text: short_name"></dd>
    <dt>Version:</dt>
    <dd data-bind="text: version_id"></dd>
    <dt>Contacts:</dt>
    <dd data-bind="with: contacts">
      <p data-bind="text: ContactPersons.ContactPerson.FirstName() + ' ' + ContactPersons.ContactPerson.LastName()"></p>
      <p data-bind="foreach: OrganizationPhones.Phone">
        <span data-bind="text: Number() + '('+ Type() +')'"></span><br />
      </p>
      <p data-bind="text: OrganizationEmails.Email"></p>
    </dd>
    <dt>Spatial Extent:</dt>
    <dd data-bind="with: points">
      <span data-bind="text: 'Point: ' + PointLatitude() + ', ' + PointLongitude()"></span>
    </dd>
    <dd data-bind="with: boxes">
      <span data-bind="text: 'Bounding Rectangle: ' + NorthBoundingCoordinate() + ', ' + WestBoundingCoordinate() + ', ' + SouthBoundingCoordinate() + ', ' + EastBoundingCoordinate()"></span>
    </dd>
    <dt>Temporal Extent:</dt>
    <dd data-bind="with: temporal.RangeDateTime">
      <p data-bind="text: BeginningDateTime() + ' to ' + EndingDateTime()"></p>
    </dd>
    <dt>Science Keywords:</dt>
    <dd data-bind="foreach: science_keywords">
      <p data-bind="text: CategoryKeyword() + ' >> ' + TopicKeyword() + ' >> ' + TermKeyword()"></p>
    </dd>
    <dt>Online Access:</dt>
    <dd data-bind="foreach: online_access_urls">
      <a data-bind="attr: {href: URL}, text: URL" target="_blank"></a>
    </dd>
    <dt>Online Resources:</dt>
    <dd data-bind="with: online_resources">
      <a data-bind="attr: {href: URL}, text: URL" target="_blank"></a>
    </dd>
    <dt>Browse Products:</dt>
    <dd data-bind="foreach: browse_images">
      <a data-bind="attr: {href: URL}" target="_blank">
        <img data-bind="attr: {src: URL}" style="height: 150px;"/>
      </a>
    </dd>
    <dt>GCMD DIF Record:</dt>
    <dd><a data-bind="attr: { href: 'http://gcmd.gsfc.nasa.gov/getdif.htm?' + associated_difs() }, text: associated_difs" target="_blank"></a></dd>
  </dl>
</section>
