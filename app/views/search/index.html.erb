  <%= form_tag('/search', method: 'get') do %>
    <p>
      <input name="keywords" data-bind="value: query.keywords, valueUpdate: 'keyup'" placeholder="Type any topic or dataset name" autofocus>
    </p>
  <% end %>
<% end %>


<section class="master-overlay-parent">
